<Window x:Class="RegistroLogin.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RegistroLogin"
        mc:Ignorable="d"
        Title="Principal" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Menu Height="18">
                <MenuItem Header="Registros">
                   
                    <MenuItem x:Name="UsuarioMenuItem" Header="Registro de Usuarios" Click="UsuarioMenuItem_Click"></MenuItem>
                   
                </MenuItem>
                <MenuItem Header="Consultas">

                    <MenuItem x:Name="UsuarioConsultasItem" Header="Consulta de Usuarios" Click="UsuarioConsultasItem_Click"></MenuItem>

                </MenuItem>


            </Menu>
        </StackPanel>
    </Grid>
</Window>
